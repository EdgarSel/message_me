<div class="ui large inverted menu">
 <div class="ui container">
      <%= link_to "Sala de chats", root_path, class: "item" %>
    <a class="item">
      Mensajes
    </a>
    <div class="right menu">
      <div class="ui dropdown item">
        Cuenta <i class="dropdown icon"></i>
        <div class="menu">
        <% if logged_in? %>
        | <%= link_to "Cerrar Sesion", logout_path, method: :delete, class: "item" %>
        <% else %>
        | <%= link_to "Iniciar Sesion", login_path, class: "item" %>
        <% end %>
        </div>
      </div>
     <% if !logged_in? %>
        <div class="item">
             <%= link_to "Iniciar Sesion", login_path, class: "ui primary button" %>
        </div>
      <%end%>
    </div>
 </div>
</div>
